{
  "name": "contentful-integration",
  "version": 2,
  "builds": [
    { "src": "src/**/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/uihook", "dest": "/src/uihook/uihook.route.js" },
    { "src": "/webhook", "dest": "/src/api/webhook.route.js" },
    { "src": "/preview", "dest": "/src/api/preview.route.js" }
  ]
}

